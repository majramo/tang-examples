<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="5">
    <test name="Test Google Safari" enabled="true">
        <parameter name="browser" value="Safari"></parameter>
        <packages>
            <package name="verification.google.*"/>
        </packages>
    </test> <!-- Test -->
    <test name="Test googledataprovider Safari" enabled="true">
        <parameter name="browser" value="Safari"></parameter>
        <packages>
            <package name="verification.googledataprovider.*"/>
        </packages>
    </test> <!-- Test -->

    <test name="Test linkedin Safari" enabled="true">
        <parameter name="browser" value="Safari"></parameter>
        <packages>
            <package name="verification.linkedin.*"/>
        </packages>
    </test> <!-- Test -->
    <test name="Test linkedindataprovider Safari" enabled="true">
        <parameter name="Safari" value="Safari"></parameter>
        <packages>
            <package name="verification.linkedinWithDataProvider.*"/>
        </packages>
    </test>

    <test name="Test ikea Safari" enabled="true">
        <parameter name="Safari" value="Safari"></parameter>
        <packages>
            <package name="verification.ikea.*"/>
        </packages>
    </test> <!-- Test -->


</suite> <!-- Suite -->
